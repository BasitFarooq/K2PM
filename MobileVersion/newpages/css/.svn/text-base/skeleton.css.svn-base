@charset "utf-8";
/* CSS Document */

#header {
    height: 57px;
}
h1#logo {
	float: left;
    height: 19px;
    margin: 0 0 0 17px;
    padding: 0;
    visibility: visible !important;
    width: 149px;	
}
h1#logo a, h1#logo a:visited, h1#logo a:hover {
	background: url(../images/logo.png) no-repeat left top;
    height: 19px;
    margin: 18px 0 6px;
    width: 149px;	
	display:block;
}
.slider {
	background:#7f080b;
	margin:0 auto;
}
#defultMenu {
    background:url(../images/defultMenu.png) repeat-x left top;
    height: 93px;
}
#defultMenu ul {
    background: url(../img/menu-bg-rpt.png) no-repeat scroll left top transparent;
}
#defultMenu ul li {
    background: url(../img/menu-sep.png) no-repeat scroll right top transparent;
    height: 93px;
}
#defultMenu ul li.noSep {
    background: none repeat scroll 0 0 transparent;
}
#defultMenu ul li a, #defultMenu li a:visited {
    font-size: 18px;
    height: 93px;
}
#defultMenu ul li a span, #defultMenu ul li a span:visited, #defultMenu ul li a span:hover {
    height: 30px;
    line-height: 19px;
    padding: 38px 7px 0;
    width: 86px;
}
#defultMenu ul li a span.arrow, #defultMenu ul li a span.arrow:visited {
    background: url(../img/small-arrows.png) no-repeat scroll center -17px transparent;
    height: 13px;
    line-height: 13px;
    margin: 16px auto 0;
}
#defultMenu ul li a:hover {
    background: url(../img/menu-hover.png) repeat-x scroll 0 0 transparent;
}
#main ul li a:hover span.arrow {
defultMenubackground: url(../img/small-arrows.png) no-repeat scroll center top transparent;
    height: 13px;
    line-height: 13px;
    margin: 16px auto 0;
}
#defultMenu ul li#current a, #defultMenu ul li#current a:visited, #defultMenu ul li#current a:hover {
    background: url(../img/menu-hover.png) repeat-x scroll 0 0 transparent;
    text-decoration: none;
}
#defultMenu ul li#current a span.arrow, #defultMenu ul li#current a:visited span.arrow, #defultMenu ul li#current a:hover span.arrow {
    background: url(../img/small-arrows.png) no-repeat scroll center top transparent;
}


#nav ul {
}
#nav li {
    height: 57px;
}
#nav li a, nav li a:visited {
    letter-spacing: -0.5px;
}

/* container starts */
.contetns {
	padding:0 10px 10px 10px;
	margin:0 auto;
}

/* user Profile */
.innermenu ul{
	background:#f1f1f1;
	border-bottom:1px solid #e5e5e5;
	padding:5px 10px 5px 10px;
}
.innermenu ul li{
	float:left;
	display:inline-block;
	height:22px; line-height:22px;
}
.innermenu ul li a{
	font-size:12px;
	display:inline-block;
	margin:0 7px;
	color:#666666;
}
.innermenu ul li a:hover{
	text-decoration:underline;
	list-style-type:none;
	color:#A71930;
}
.innermenu ul li.sep{
	background:url(../images/sep.jpg) no-repeat left top;
	height:22px;
	line-height:22px;
	width:1px;
	float:left;	
}

#ageRegister {
	margin:20px 0;
}

/* userProfile */
#userProfile { 

}
.profilePIc {
	border:1px solid #cccccc;
	padding:1px;
	width:50px;
	height:50px;	
}
.profilePIc img { width:50px; height:50px;}
.userLinks {	
}
.innerwrapper {
	padding:10px 0;	
}


/* footer */
#footerWrap {
	background:#8a090c;
    padding:10px 0;
}
footer {
    margin: 0 12px;
    font-size: 12px;
    padding: 12px 0 2px;
	text-align:center;
}
footer a, footer a:visited {
    color: #ffffff;
    display: inline-block;
    height: 15px;
    line-height: 15px;
    margin: 0 5px 0 0;
    padding: 3px 10px;
    text-decoration: none;
    text-transform: uppercase;
}
footer a:hover {
    background:#520507;
    color: #FFFFFF;
    text-decoration: none;
}
.dots {
    background: url(../images/dots.gif) repeat-x scroll left bottom transparent;
    font-size: 0;
    height: 2px;
    margin: 5px 7px 7px 6px;
}
.copyright {
    color: #ffffff;
    float: left;
    font-size: 10px;
    height: 20px;
    line-height: 10px;
    margin-left: 6px;
    text-transform: uppercase;
    width: 230px;
}
.socialIcons {
    float: right;
    height: 20px;
    margin-right: 7px;
	line-height:26px;
}
.note {
	background-color: #99BAF6;
    border: 1px solid #363535;
    margin: 5px 0 15px;
    padding: 10px;
	font-size:11px;
}
.note ul { list-style-type:decimal !important; color:#000000; margin:0 0 10px 0;}
.note ul li { font-weight:normal; list-style-type:decimal !important; display:block; margin:0 0 5px 0;}
 
.sportSubNav {
    margin-bottom: 15px;
    width: 33%;	
}
.sportSubNav a {
    background: url(../images/sports-to-bg.png) no-repeat right center #A71930;
    font-size: 22px;
    width: 82%;
}
.sportSubNav a:hover {
    background: url(../images/sports-to-bg.png) no-repeat right center #721120;
}
.sportSubNav a span {
    width: 150px;
}
